{
  "name": "@apideck/node",
  "version": "0.1.0",
  "description": "Apideck API wrapper",
  "keywords": [
    "apideck",
    "api",
    "unified apis",
    "crm"
  ],
  "homepage": "https://apideck.com",
  "author": "Apideck <support@apideck.com> (https://apideck.com/)",
  "contributors": [
    "Geert Wille <geert@apideck.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/apideck-libraries/apideck-node.git"
  },
  "bugs:": "https://github.com/apideck-libraries/apideck-node/issues",
  "engines": {
    "node": ">=10"
  },
  "module": "dist/apideck.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "release": "np"
