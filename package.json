    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "license": "Apache-2.0",
  "peerDependencies": {},
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.8",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "np": {
    "yarn": true,
    "tests": false
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
